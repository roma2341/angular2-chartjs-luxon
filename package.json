{
  "name": "angular2-chartjs-luxon",
  "version": "0.5.1",
  "description": "Chart.js component for Angular2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/* && ngc",
    "build:demo": "cd dev && ngc",
    "prepublish": "npm run build",
    "build-stats": "ng build app  --prod --aot --stats-json & webpack-bundle-analyzer dist/app/stats.json",
    "start": "webpack-dev-server --progress --profile --colors --watch --display-error-details --display-cached --content-base dev/"
  },
  "peerDependencies": {},
  "dependencies": {
    "chart.js": "^2.8.0",
